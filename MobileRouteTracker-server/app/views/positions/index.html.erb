<h1>Listing positions</h1>

<table>
  <tr>
    <th>Account</th>
    <th>longitude</th>
    <th>latitude</th>
    <th>altitude</th>
    <th>accuracy</th>
    <th>bearing</th>
    <th>time</th>
    <th>description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @positions.each do |position| %>
  <tr>
    <td><%= position.account.id %></td>
    <td><%= position.longitude %></td>
    <td><%= position.latitude %></td>
    <td><%= position.altitude %></td>
    <td><%= position.accuracy %></td>
    <td><%= position.bearing %></td>
    <td><%= position.time %></td>
    <td><%= position.description %></td>
    <td><%= link_to 'Show', position %></td>
    <td><%= link_to 'Edit', edit_position_path(position) %></td>
    <td><%= link_to 'Destroy', position, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Position', new_position_path %>
